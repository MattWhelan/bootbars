{
  "name": "consolable",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "body-parser": "~1.6.6",
    "cookie-parser": "~1.3.2",
    "debug": "~1.0.4",
    "express": "~4.8.6",
    "jade": "~1.5.0",
    "jquery": "^2.1.1",
    "morgan": "~1.2.3",
    "serve-favicon": "~2.0.1",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "browserify": "^5.11.0",
    "browserify-shim": "^3.7.0",
    "gulp": "^3.8.7",
    "gulp-browserify": "^0.5.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-sass": "^0.7.3",
    "jadeify": "^2.5.1"
  },
  "browser": {
    "bootstrap": "./public/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "jadeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "bootstrap": {
      "depends": "jquery:jQuery",
      "exports": "bootstrap"
    }
  }
}
